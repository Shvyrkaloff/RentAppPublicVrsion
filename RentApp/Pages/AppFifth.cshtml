}
<section>
        <div class="container  mt-5 mb-5 d-flex">
            <div class="row">
                <div class="col-8">
                    <div id="carouselId" class="carousel slide" data-bs-ride="carousel">
                        <ol class="carousel-indicators">
                            <li data-bs-target="#carouselId" data-bs-slide-to="0" class="active" aria-current="true" aria-label="First slide"></li>
                            <li data-bs-target="#carouselId" data-bs-slide-to="1" aria-label="Second slide"></li>
                            <li data-bs-target="#carouselId" data-bs-slide-to="2" aria-label="Third slide"></li>
                        </ol>
                        <div class="carousel-inner" role="listbox">
                            <div class="carousel-item active">
                                <img src="https://marvel-b1-cdn.bc0a.com/f00000000254107/cdn.tfc.com/marketing/files/building_images/4545-Center-Blvd_Lobby_Front-Desk.jpg" alt="First slide" width="800">
                            </div>
                            <div class="carousel-item">
                                <img src="https://marvel-b1-cdn.bc0a.com/f00000000254107/cdn.tfc.com/marketing/files/building_images/4545-Center-Blvd_Lobby_Front-Desk.jpg" alt="Second slide" width="800">
                            </div>
